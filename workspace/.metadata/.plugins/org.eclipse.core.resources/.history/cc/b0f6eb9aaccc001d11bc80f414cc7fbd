package com.project.odw.like.dao;

import java.util.HashMap;

import org.apache.ibatis.session.SqlSession;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import com.project.odw.like.dto.LikeDto;

@Repository
public class LikeDaoImpl implements LikeDao {
	
	@Autowired
	SqlSession sqlSession;

	@Override
	public void insertLike(HashMap<String, Object> likeData) throws Exception {
		sqlSession.insert("mapper.like.insertLike", likeData);
	}

	@Override
	public int likeChk(LikeDto likeDto) throws Exception {
		return sqlSession.select("mapper.like.likeChk", likeDto);
	}

}
