package com.project.odw.like.service;

import java.util.HashMap;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.project.odw.like.dao.LikeDao;

@Service
public class LikeServiceImpl implements LikeService {
	
	@Autowired
	private LikeDao likeDao;

	@Override
	public void likeSend(String likeSend, String likeReceive) throws Exception {
		
		HashMap<String, Object> likeData = new HashMap<>();
		System.out.println(likeData.get("likeSend"));
		System.out.println(likeData.get("likeReceive"));
		likeDao.insertLike(likeData);
	}

}
